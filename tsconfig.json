{
    "compilerOptions": {
      "noImplicitAny": true,
      "module": "ESNext",
      "target": "ES6",
      "jsx": "react-jsx",
      "allowJs": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strictNullChecks":true,
      "baseUrl": "./src",
	  "skipLibCheck": true,
    },

    "exclude": ["node_modules", "build"],

  }